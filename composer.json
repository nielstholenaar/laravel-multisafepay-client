{
    "name": "ntholenaar/laravel-multisafepay-client",
    "type": "library",
    "description": "Laravel bridge for the ntholenaar/multisafepay-client package.",
    "keywords": [
        "multisafepay",
        "client",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Tholenaar",
            "email": "info@123quality.nl",
            "homepage": "http://123quality.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ntholenaar\\LaravelMultiSafepayClient\\" : "src/"
        }
    },
    "require": {
        "ntholenaar/multisafepay-client": "^0.2",
        "illuminate/contracts": "~5.5"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.1.1",
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "~6.0"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ntholenaar\\LaravelMultiSafepayClient\\Providers\\MultiSafepayClientServiceProvider"
            ],
            "aliases": {
                "MultiSafepayClient": "Ntholenaar\\LaravelMultiSafepayClient\\Facades\\MultiSafepayClient"
            }
        }
    }
}
